{
  "name": "hasanparasteh/helmet",
  "description": "Helmet.js-style security headers middleware for ReactPHP HTTP.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Hasan Parasteh",
      "email": "hasanparasteh@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "react/http": "^1.9",
    "react/promise": "^3.0",
    "psr/http-message": "^1.1 || ^2.0",
    "nyholm/psr7": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "HP\\Helmet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HP\\Helmet\\Tests\\": "tests/"
    }
  },
  "keywords": [
    "reactphp",
    "security",
    "helmet",
    "middleware",
    "headers",
    "csp",
    "hsts"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
